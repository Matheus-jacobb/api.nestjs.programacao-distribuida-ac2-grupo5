{
  "name": "api-eventos",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:prod": "rimraf dist && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node src/index.ts",
    "start:dev": "rimraf dist && cross-env TZ=UTC nest start --watch",
    "start:debug": "rimraf dist && cross-env TZ=UTC nest start --debug --watch --preserveWatchOutput",
    "start:prod": "cross-env TZ=UTC node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "npm run test:e2e",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./jest-e2e.json",
    "typeorm:cli": "npm run build:prod && ts-node ./node_modules/typeorm/cli -f ormconfig.js",
    "migration": "npm run typeorm:cli -- migration:run",
    "add-migration": "npm run typeorm:cli -- migration:generate -n",
    "revert-migration": "npm run typeorm:cli -- migration:revert",
    "deploy:heroku": "cd .. && git subtree push --prefix backend heroku master",
    "deploy:aws": "node deploy/aws.js",
    "joga10": "ts-node -r tsconfig-paths/register src/console/index.ts",
    "joga10:debug": "node --inspect-brk dist/src/console/index.js"
  },
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@nestjs/common": "^7.4.4",
    "@nestjs/core": "^7.4.4",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/swagger": "^4.6.0",
    "@nestjs/typeorm": "^7.1.3",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-typeorm": "^4.6.2",
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "connect-timeout": "^1.9.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.2",
    "express-rate-limit": "^5.1.3",
    "helmet": "^4.1.0",
    "jwt-decode": "^2.2.0",
    "moment-timezone": "^0.5.31",
    "nodemailer": "^6.4.11",
    "omit-deep": "^0.3.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.3",
    "print-message": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.2",
    "sharp": "^0.26.2",
    "sqlite3": "^5.0.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "~0.2.25",
    "typeorm-transactional-cls-hooked": "^0.1.12",
    "uuid": "^8.3.0",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.1",
    "@nestjs/testing": "^7.4.4",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.8",
    "@types/express-rate-limit": "^5.1.0",
    "@types/jest": "^26.0.12",
    "@types/luxon": "^1.21.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^12.12.55",
    "@types/passport-facebook-token": "^0.4.34",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/pluralize": "0.0.29",
    "@types/prompts": "^2.0.9",
    "@types/sharp": "^0.26.0",
    "@types/supertest": "^2.0.8",
    "commander": "^6.2.1",
    "jest": "^26.0.1",
    "nestjs-console": "^4.0.0",
    "param-case": "^3.0.4",
    "pascal-case": "^3.1.2",
    "pluralize": "^8.0.0",
    "prettier": "^2.0.5",
    "prompts": "^2.4.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
